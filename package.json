{
  "name": "banana-cms",
  "version": "1.0.0",
  "description": "BananaCMS Frontend Build Tools",
  "scripts": {
    "build-css": "tailwindcss -i ./static/css/tailwind.input.css -o ./static/css/tailwind.min.css --minify --postcss",
    "watch-css": "tailwindcss -i ./static/css/tailwind.input.css -o ./static/css/tailwind.min.css --watch --postcss",
    "build": "npm run build-css"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "postcss-custom-properties": "^13.3.2",
    "autoprefixer": "^10.4.16"
  },
  "keywords": ["cms", "video", "tailwindcss"],
  "author": "BananaCMS",
  "license": "MIT"
}